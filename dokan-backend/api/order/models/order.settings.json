{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "total": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "payment_required",
        "payed",
        "refunded",
        "canceled"
      ],
      "default": "payment_required"
    },
    "code": {
      "type": "string"
    }
  }
}
